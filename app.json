{
  "expo": {
    "name": "BIMS",
    "slug": "bims",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/bims.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.exco.connectmobile"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/bims.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.BIMS.app"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/benuelogo2.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "b5c40c62-a0d5-4335-807d-d5b8c775b527"
      }
    },
    "owner": "abyemj"
  }
}
